<div class="panel panel-primary">
  <div class="panel-heading">
      <h3 class="panel-title">Log in Page</h3>
  </div>
<div class="row">
  <div class= "col-md-2"></div>
  <div class= "col-md-8">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

   <div class="panel-body">
    <form class="form-horizontal">
    <div class="form-group">
      <div class="col-lg-10">
        <%= f.email_field :email, id: "email", class:"form-control floating-label", placeholder: "Email" %>
      </div>
    </div><br>
    <form class="form-horizontal">
    <div class="form-group">
      <div class="col-lg-10">
        <%= f.password_field :password, id: "password" , autocomplete: "off", class:"form-control floating-label", placeholder: "Password" %>
      </div><br>
    </div><br>

    <% if devise_mapping.rememberable? -%>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end -%>
  <div class="actions">
    <%= f.submit "Log In", class: "btn btn-success pull-left" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
